    <table width="778" border="0" cellpadding="0" cellspacing="0" bgcolor="#FFFFFF" class="border">
        <tr>
            <td align="center">
                <table width="766" border="0" cellpadding="0" cellspacing="0" bgcolor="#ccccd4">
                    <tr>
                        <td align="left" height="10">
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
    </table>
    <!--Footer-->
    <table width="778" border="0" cellpadding="0" cellspacing="6" bgcolor="#FFFFFF" class="border">
        <tr>
            <td>
                <table width="100%" border="0" cellpadding="0" cellspacing="0" class="navLight">
                    <tr>
                        <td height="42">
                            <table width="100%" border="0" cellspacing="0" cellpadding="2">
                                <tr>
                                    <td>
                                        Copyright 2008 MyPetShop
                                    </td>
                                </tr>
                            </table>
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
    </table>
