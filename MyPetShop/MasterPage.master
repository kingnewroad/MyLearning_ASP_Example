                                    <td>
                                        <asp:TextBox ID="txtSearch" runat="server"></asp:TextBox>
                                    </td>
                                    <td>
                                        <asp:ImageButton ID="imgbtnSearch" runat="server" ImageUrl="~/img/searchbutton.gif"
                                            CausesValidation="False" OnClick="imgbtnSearch_Click" />
                                    </td>
                                    <td>
                                        <asp:ImageButton ID="imgbtnXml" runat="server" ImageUrl="~/img/xml.gif" />
                                    </td>
                                </tr>
                                <tr>
                                    <td colspan="5" bgcolor="#ebeadd">
                                    </td>
                                </tr>
                            </table>
                        </td>
                    </tr>
                    <tr>
                        <td bgcolor="#FFFFFF" height="6">
                        </td>
                    </tr>
                </table>
                <table width="100%" border="0" cellspacing="6" cellpadding="0">
                    <tr>
                        <td>
                            <asp:ContentPlaceHolder ID="ContentPlaceHolder2" runat="server">
                            </asp:ContentPlaceHolder>
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
    </table>
    <!--Body2-->
    <table width="778" border="0" cellpadding="0" cellspacing="0" bgcolor="#FFFFFF" class="border">
        <tr>
            <td align="center">
                <table width="766" border="0" cellpadding="0" cellspacing="0" bgcolor="#ccccd4">
                    <tr>
                        <td align="left" height="10">
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
    </table>
    <table width="778" border="0" cellpadding="0" cellspacing="6" class="border">
        <tr>
            <td width="227" valign="top">
                <asp:ContentPlaceHolder ID="ContentPlaceHolder3" runat="server">
                </asp:ContentPlaceHolder>
            </td>
            <td width="275" valign="top">
                <asp:ContentPlaceHolder ID="ContentPlaceHolder4" runat="server">
